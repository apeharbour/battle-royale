{
  "name": "newSubgraph",
  "license": "MIT",
  "version": "1.0.0",
  "scripts": {
    "codegen": "graph codegen",
    "build": "graph build",
    "deploy": "graph deploy --node https://api.thegraph.com/deploy/ newSubgraph/newSubgraph",
    "create-local": "graph create --node http://localhost:8020/ newSubgraph/newSubgraph",
    "remove-local": "graph remove --node http://localhost:8020/ newSubgraph/newSubgraph",
    "deploy-local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 newSubgraph/newSubgraph --version-label v1 --network localhost",
    "deploy:local": "pnpm run codegen && pnpnm run create-local && pnpm run deploy-local",
    "test": "graph test"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "^0.68.3",
    "@graphprotocol/graph-ts": "^0.33.0"
  },
  "devDependencies": {
    "matchstick-as": "0.6.0"
  }
}
